\r\n  }\r\n  \r\n  svg,\r\n  img {\r\n    display: block;\r\n    max-width: 100%;\r\n  }\r\n  \r\n  input {\r\n    cursor: pointer;\r\n  }\r\n  \r\n  \r\n  a {\r\n    display: inline-block;\r\n    text-decoration: none;\r\n  }\r\n  \r\n  ul {\r\n    list-style: none;\r\n  }","sass\\layout\\_about.scss":".about__preTitle {\r\n  font-family: $fontTitan;\r\n  @include fontStyle(400, 18, 20.61, 2);\r\n  color: $pre-title-color;\r\n  text-align: center;\r\n}\r\n.about__title {\r\n  font-family: $fontTitan;\r\n  @include fontStyle(400, 34, 38.93, 4);\r\n  color: $accent-color;\r\n  text-align: center;\r\n}\r\n\r\n.about__text {\r\n  font-family: $fontDM;\r\n  @include fontStyle(500, 12, 19.44, 2);\r\n  color: $main-text-color;\r\n}\r\n.about__text--color {\r\n  color: $light-text-color;\r\n}\r\n\r\n.about__btn {\r\n  font-family: $fontDM;\r\n  @include fontStyle(700, 14, 18.23, 2);\r\n  color: $accent-color;\r\n}\r\n","sass\\utils\\_mixims.scss":"$fontDM: 'DM Sans', sans-serif;\r\n$fontTitan: 'Titan One', cursive;\r\n\r\n@mixin fontStyle($fontWeight, $fontSize, $liHeig, $letSpac) {\r\n  font-weight: $fontWeight;\r\n  font-size: (#{$fontSize * 1px});\r\n  line-height: (#{$liHeig / $fontSize});\r\n  letter-spacing: (#{$letSpac * 0.01em});\r\n}\r\n\r\n// break point\r\n@mixin forSize($range) {\r\n  $mobile: 480px;\r\n  $tablets: 768px;\r\n  $desktop: 1200px;\r\n\r\n  @if $range == 'maxtablets' {\r\n    @media screen and (max-width: #{$tablets - 0.02px}) {\r\n      @content;\r\n    }\r\n  } @else if ($range == 'minimobile') {\r\n    @media screen and  (max-width : #{$mobile - 0.02px}) {\r\n      @content;\r\n    }\r\n  } @else if ($range == 'mobile') {\r\n    @media screen and  (min-width : #{$mobile}) {\r\n      @content;\r\n    }\r\n  } @else if ($range == 'tablets') {\r\n    @media screen and (min-width: #{$tablets}) {\r\n      @content;\r\n    }\r\n  } @else if ($range == 'desktop') {\r\n    @media screen and (min-width: #{$desktop}) {\r\n      @content;\r\n    }\r\n  } @else if ($range == 'maxdesktop') {\r\n    @media screen and (max-width: #{$desktop} - 0.02) {\r\n      @content;\r\n    }\r\n  } @else if ($range == 'minmaxtablets') {\r\n    @media screen and (min-width: #{$tablets}) and (max-width: 1199px) {\r\n      @content;\r\n    }\r\n  }\r\n}\r\n","sass\\utils\\_colors-vars.scss":"$main-color: #ffffff;\r\n$accent-color: #d41443;\r\n$hero-bg-color: #ffb8ca;\r\n$pre-title-color: #ffa5ba;\r\n$ice-coffee-color: #f0d1a5;\r\n$milkshake-color: #c2e297;\r\n$light-bg-color: #fff5f6;\r\n$main-text-color: #000000;\r\n$light-text-color: #907e82;\r\n$slider-light-color: #d9d9d9;\r\n$slider-accent-color: #d31342;\r\n$review-bg-color: #fafafa;\r\n$pseudo-color: #e1e1e1;\r\n$review-dots-color: #c7133e;\r\n$socials-color: #fea5bb;\r\n$button-hover-color: #ecebeb;\r\n$menu-bg-color: #e17992;\r\n$menu-list-border-color: #e18298;\r\n","sass\\layout\\_reviews.scss":".section-reviews {\r\n  padding: 80px 15px 86px 15px;\r\n  margin: 0 auto;\r\n  padding-left: 15px;\r\n  padding-right: 15px;\r\n\r\n  @media screen and (min-width: 320px) {\r\n    max-width: 320px;\r\n  }\r\n  @media screen and (min-width: 480px) {\r\n    max-width: 480px;\r\n  }\r\n  @media screen and (min-width: 768px) {\r\n    max-width: 768px;\r\n  }\r\n  @media screen and (min-width: 1200px) {\r\n    max-width: 1200px;\r\n  }\r\n}\r\n\r\n.visually-hidden {\r\n  position: absolute;\r\n  width: 1px;\r\n  height: 1px;\r\n  margin: -1px;\r\n  border: 0;\r\n  padding: 0;\r\n\r\n  white-space: nowrap;\r\n  clip-path: inset(100%);\r\n  clip: rect(0 0 0 0);\r\n  overflow: hidden;\r\n}\r\n.section-reviews__img {\r\n  display: block;\r\n  /* width: 100%; */\r\n  width: 85px;\r\n  height: 85px;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n}\r\n.section-reviews__text {\r\n  font-family: DM Sans;\r\n  font-style: normal;\r\n  font-weight: 500;\r\n  font-size: 14px;\r\n  line-height: 216%;\r\n  text-align: center;\r\n  letter-spacing: 0.04em;\r\n  color: #000000;\r\n  border-bottom: 1px solid #e1e1e1;\r\n}\r\n.reviews-nav {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"0c9e6084c2e8a93f558ae8a8e8cf05e3","cacheData":{"env":{}}}